<script type="text/javascript">
	
	var myJSONObject5 = 
	{
		"test" : "function(arg) { alert('메소드 테스트 : ' + arg) }"
	}
	
	var t = myJSONObject5.test;
	
</script>

<form>

	<input type="button" value="click" onclick="t('홍길동')">
	
	<input type="button" value="click" onclick="eval('var a=' + myJSONObject5.test); a('이지혜')">

</form>