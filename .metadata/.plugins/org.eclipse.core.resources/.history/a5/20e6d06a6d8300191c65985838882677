<script type="text/javascript">

	var obj4 = 
	{
		"test" : null
	}

</script>

<form>
	
	<input type="button" value="click1" onclick="alert(obj4)">
	<input type="button" value="click2" onclick="alert(typeof obj4)">
	
	<input type="button" value="click3" onclick="alert(obj4.test)">
	<!-- null -->
	<input type="button" value="click4" onclick="alert(typeof obj4.test)">
	<!-- object -->
	
</form>